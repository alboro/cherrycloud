{
    "name": "Fractal Note",
    "description": "Online editor of note trees for Nextcloud/Owncloud server with Cherrytree (*.ctb) support",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Alexander Demchenko",
            "email": "a.demchenko@aldem.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/alboro/fractalnote/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        { "type": "vcs", "url": "https://github.com/alboro/jstree-bootstrap-theme.git" },
        { "type": "vcs", "url": "https://github.com/alboro/handlebars.js.git" }
    ],
    "require": {
        "php": ">=5.6.0",
        "vakata/jstree": ">=3.3.4",
        "orangehill/jstree-bootstrap-theme": "dev-master#0443c0efc590b382a8e0ad6a521c7ca8635dd23e",
        "components/handlebars.js": "dev-master#b9bb577d89558cc54edcb6f40f27bf5b264d78ed"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "OCA\\FractalNote\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
