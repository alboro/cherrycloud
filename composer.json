{
    "name": "Fractal Note",
    "description": "Online editor of note trees for Nextcloud/Owncloud server with Cherrytree (*.ctb) support",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Alexander Demchenko",
            "email": "a.demchenko@aldem.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/alboro/fractalnote/issues"
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/alboro/jstree-bootstrap-theme.git", "no-api": true },
        { "type": "vcs", "url": "https://github.com/components/handlebars.js.git", "no-api": true }
    ],
    "require": {
        "php": ">=5.6.0",
        "vakata/jstree": ">=3.3.4",
        "orangehill/jstree-bootstrap-theme": "dev-master#fbdf58ea6372302a8c4546f254e2c16c2ace9aff",
        "components/handlebars.js": "dev-master#b9bb577d89558cc54edcb6f40f27bf5b264d78ed",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "codeception/codeception": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "OCA\\FractalNote\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
