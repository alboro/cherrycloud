<?xml version="1.0"?>
<info>
    <id>cherrycloud</id>
    <name>CherryCloud</name>
    <description>CherryTree file editor</description>
    <licence>MIT</licence>
    <author>Alexander Demchenko</author>
    <version>0.0.3</version>
    <namespace>CherryCloud</namespace>
    <category>office</category>
    <category>organization</category>
    <category>tools</category>
    <website>https://github.com/alboro/cherrycloud</website>
    <dependencies>
        <php min-version="5.6" max-version="7"/>
        <owncloud min-version="8.1" max-version="10" />
        <nextcloud min-version="9" max-version="12" />
    </dependencies>
    <repair-steps>
        <install>
            <step>OCA\CherryCloud\Migration\AddMimetypeToFilecache</step>
        </install>
        <uninstall>
            <step>OCA\CherryCloud\Migration\RemoveMimetypeFromFilecache</step>
        </uninstall>
    </repair-steps>
</info>